  <DataSets>
    <DataSet Name="MonthlySales">
      <Query>
        <DataSourceName>MyStore_DataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ItemName">
            <Value>=Parameters!ItemName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ReportMonth">
            <Value>=Parameters!ReportMonth.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>Select d.First_Of_Month 'Sales Month' ,
i.Item_Name,
i.Item_Section,
i.Item_Price,
e.employee_name
 
from items i 
inner join sales s on i.Item_ID=s.Item_ID 
inner join Dim_Date d on s.Sale_Date=d.Date_Key 
inner join Employee e on s.employee_ID=e.employee_ID
where i.Item_Name in(@ItemName)
and d.first_of_Month=@ReportMonth</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Sales_Month">
          <DataField>Sales Month</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Item_Name">
          <DataField>Item_Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Item_Section">
          <DataField>Item_Section</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Item_Price">
          <DataField>Item_Price</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="employee_name">
          <DataField>employee_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Prompt_Item_Name">
      <Query>
        <DataSourceName>MyStore_DataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ItemSection">
            <Value>=Parameters!ItemSection.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>Select distinct Item_Name
from Items
where Item_Section in (@ItemSection)
order by Item_name</CommandText>
      </Query>
      <Fields>
        <Field Name="Item_Name">
          <DataField>Item_Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Prompt_Report_Month">
      <Query>
        <DataSourceName>MyStore_DataSource</DataSourceName>
        <CommandText>Select first_of_month,
datename(Month, date_key) + ' ' + datename(year, date_key) Month_Year 
from Dim_Date
where first_of_month&lt;= getDate()
order by first_of_month desc</CommandText>
      </Query>
      <Fields>
        <Field Name="first_of_month">
          <DataField>first_of_month</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Month_Year">
          <DataField>Month_Year</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Prompt_Item_Section">
      <Query>
        <DataSourceName>MyStore_DataSource</DataSourceName>
        <CommandText>Select distinct Item_section
from Items
order by Item_Section</CommandText>
      </Query>
      <Fields>
        <Field Name="Item_section">
          <DataField>Item_section</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
